{
  "name": "kms-ipmon",
  "version": "1.2.0",
  "description": "Server Public Ip Checker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "test": "mocha --reporter spec",
    "preversion": "npm run lint",

  "dependencies": {
    "axios": "^0.28.0",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@types/node": "16.18.3",
    "chai": "4.3.6",
    "http-status-codes": "2.2.0",
    "mocha": "9.2.2",
    "prettier": "2.8.8",
    "ts-node": "10.9.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",

  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "chai": "4.3.4",
    "http-status-codes": "2.1.4",
    "mocha": "9.1.3",
    "prettier": "2.4.1",
    "ts-node": "10.4.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.4.4"
